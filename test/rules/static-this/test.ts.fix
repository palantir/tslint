class StaticThis {
    static value = 0;
    static staticValue = StaticThis.value;

    static getTypeOf() {
        return StaticThis;
    }

    private _value = 5;

    getValue() {
        return this._value;
    }

    get length() {
        return this._value;
    }

    set length(value) {
        this._value = value;
    }
}

class Child extends StaticThis {
    static word = 'Hello';
    static staticValue = Child.word;

    static getTypeOf() {
        return Child;
    }
}

const AnonymClass = class {
    static getTypeOf() {
        return this;
    }
}

const NamedClass = class Name {
    static getTypeOf() {
        return Name;
    }
}
