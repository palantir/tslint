{
  "rules": {
    "curly": true,
    "eofline": true,
    "indent": [true, "spaces"],
    "max-line-length": [true, 120],
    "no-bitwise": true,
    "no-unused-expression": true,
    "quotemark": [true, "double"],
    "semicolon": [true, "always"],
    "whitespace": [true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type",
      "check-typecast"
    ]
  }
}
