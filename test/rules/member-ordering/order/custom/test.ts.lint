class Good {
    constructor() {}
    bar = 5;
    static foo() {}
    private z = 12;
    private static zz = 10;
}

class Bad {
    constructor() {}
    private static a() {}
    private c = 2;
    static b() {}
    ~~~~~~~~~~~~~ [Declaration of public static method not allowed after declaration of private instance field. Instead, this should come at the beginning of the class/interface.]
}
